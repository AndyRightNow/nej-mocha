{
  "name": "nej-mocha",
  "version": "4.1.0",
  "description": "A test framework specifically for NEJ module system users.",
  "main": "./bin/nej-mocha",
  "scripts": {
    "start": "node ./bin/nej-mocha",
    "postinstall": "bower install",
    "patch:publish": "npm version patch && npm publish",
    "major:publish": "npm version major && npm publish",
    "minor:publish": "npm version minor && npm publish",
    "build": "gulp browserify",
    "test": "mocha --timeout=30000 --recursive && npm start",
    "lint": "eslint **/**/*.js"
  },
  "bin": {
    "nej-mocha": "./bin/nej-mocha"
  },
  "engines": {
    "node": "^5.12.0"
  },
  "author": "AndyRightNow",
  "license": "MIT",
  "repository": "https://github.com/AndyRightNow/nej-mocha",
  "bugs": "https://github.com/AndyRightNow/nej-mocha/issues",
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cors": "^2.8.1",
    "debug": "^2.6.1",
    "ejs": "^2.5.6",
    "esprima": "^3.1.3",
    "express": "^4.15.0",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nightmare": "^2.10.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-rename": "^1.2.2",
    "nyc": "^10.2.0"
  }
}
