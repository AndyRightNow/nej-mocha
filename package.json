{
  "name": "nej-mocha",
  "version": "4.2.3",
  "description": "A test framework specifically for NEJ module system users.",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "patch:publish": "npm version patch && npm publish",
    "major:publish": "npm version major && npm publish",
    "minor:publish": "npm version minor && npm publish",
    "build": "gulp browserify",
    "test:unit": "mocha --timeout=30000 --recursive ./test/unit/",
    "test:e2e": "mocha --timeout=30000 --recursive ./test/e2e/spec/**/**/*.spec.js",
    "test:cli": "node ./bin/nej-mocha --configPath=test/e2e/fixtures/nej-mocha.conf.js && node ./bin/nej-mocha --configPath=test/e2e/fixtures/nej-mocha-not-closing.conf.js",
    "test": "npm run test:unit && npm run test:e2e && npm run test:cli",
    "test:cover": "nyc npm run test",
    "test:unit:cover": "nyc npm run test:unit",
    "test:e2e:cover": "nyc npm run test:e2e",
    "lint": "eslint **/**/*.js",
    "pre-push": "npm run build && npm run lint && npm run test:cover"
  },
  "bin": {
    "nej-mocha": "./bin/nej-mocha"
  },
  "engines": {
    "node": "5 || 6 || 7"
  },
  "devEngines": {
    "node": "5 || 6 || 7"
  },
  "author": "AndyRightNow",
  "license": "MIT",
  "repository": "https://github.com/AndyRightNow/nej-mocha",
  "bugs": "https://github.com/AndyRightNow/nej-mocha/issues",
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cors": "^2.8.1",
    "debug": "^2.6.1",
    "ejs": "^2.5.6",
    "esprima": "^3.1.3",
    "express": "^4.15.0",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nightmare": "^2.10.0",
    "traverse": "^0.6.6",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-rename": "^1.2.2",
    "nyc": "^10.2.0",
    "standard": "^10.0.2"
  }
}
